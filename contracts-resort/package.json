{
  "name": "nwl-contracts-resort",
  "version": "0.0.2",
  "private": true,
  "license": "UNLICENSED",
  "type": "commonjs",
  "scripts": {
    "check:circdep": "dpdm --no-warning --no-tree scripts/deploy/dev.ts scripts/deploy/prod.ts",
    "check:ts": "tsc --noemit",
    "precompile": "rimraf ./build/",
    "compile": "hardhat compile",
    "deploy:dev": "hardhat run scripts/deploy/dev.ts",
    "deploy:prod": "hardhat run scripts/deploy/prod.ts",
    "eslint": "eslint . --max-warnings 0",
    "flatten": "hardhat flatten > _flatten.sol",
    "lint": "pnpm check:ts && pnpm check:circdep && pnpm eslint",
    "mock:PickRates": "hardhat run scripts/mocking/pickRates.ts",
    "mock:TrustedValidator": "hardhat run scripts/mocking/trustedValidator.ts",
    "sort": "sort-package-json",
    "pretest": "pnpm compile",
    "test": "hardhat test"
  },
  "dependencies": {
    "@nightworkerslabs/nft-assets": "../nft-assets/",
    "@nightworkerslabs/offchain-resort": "../offchain-resort",
    "@nomicfoundation/hardhat-verify": "^2.0.12",
    "@nomiclabs/hardhat-ethers": "^2.0.6",
    "@nomiclabs/hardhat-waffle": "^2.0.3",
    "@openzeppelin/contracts": "^4.7.0",
    "@openzeppelin/hardhat-upgrades": "^1.19.0",
    "ethers": "^5.6.9",
    "fs-extra": "^11.2.0",
    "hardhat": "^2.9.9",
    "hardhat-abi-exporter": "^2.10.0",
    "hardhat-exposed": "^0.2.3",
    "hardhat-gas-reporter": "^1.0.8",
    "log-update": "4.0.0",
    "svg-parser": "^2.0.4",
    "tsconfig-paths": "^4.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@types/chai": "^4.3.1",
    "@types/fs-extra": "^11.0.4",
    "@types/mocha": "^9.1.1",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "chai": "^4.3.6",
    "chai-bignumber": "^3.0.0",
    "dpdm-fast": "^1.0.7",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-chai-friendly": "^1.1.0",
    "eslint-plugin-no-relative-import-paths": "^1.6.1",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "mocha": "^10.0.0",
    "mocha-steps": "^1.3.0",
    "prettier": "^3.5.3",
    "sort-package-json": "^3",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.43.0"
  },
  "packageManager": "pnpm@9.15.0",
  "engines": {
    "node": ">=22"
  }
}
